{
  "manifest_version": 3,
  "name": "YouTube Speed Control",
  "icons": {
    "120": "120.png"
  },
  "version": "1.3.0",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/watch?v=*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}